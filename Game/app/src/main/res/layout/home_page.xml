<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/di_background">

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@drawable/di_rect_rounded_button"
        android:onClick="Play_Card_Game"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_play_game" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="181dp"
        android:layout_height="75dp"
        android:layout_marginTop="75dp"
        android:background="@drawable/di_rect_rounded_text"
        android:gravity="center"
        android:onClick="Play_Cup_Game"
        android:text="Find the Coin"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@drawable/di_rect_rounded_button"
        android:onClick="Play_Pattern_Game"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/music_game_name_txt"
        app:layout_constraintEnd_toStartOf="@+id/music_game_name_txt"
        app:layout_constraintTop_toTopOf="@+id/music_game_name_txt"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/logo_pattern_game" />

    <ImageButton
        android:id="@+id/usic_game_btn"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@drawable/di_rect_rounded_button"
        android:onClick="Play_Pattern_Game"
        app:layout_constraintBottom_toBottomOf="@+id/music_game_name_txt"
        app:layout_constraintStart_toEndOf="@+id/music_game_name_txt"
        app:layout_constraintTop_toTopOf="@+id/music_game_name_txt"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_play_game" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@drawable/di_rect_rounded_button"
        android:onClick="Play_Card_Game"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/logo_matching_game" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@drawable/di_rect_rounded_button"
        android:onClick="Play_Cup_Game"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintEnd_toStartOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="@+id/textView8"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/logo_brain" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="181dp"
        android:layout_height="75dp"
        android:layout_marginTop="75dp"
        android:background="@drawable/di_rect_rounded_text"
        android:gravity="center"
        android:onClick="Play_Card_Game"
        android:text="Match the Cards "
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageButton2"
        app:layout_constraintStart_toStartOf="@+id/imageView12"
        app:layout_constraintTop_toBottomOf="@+id/music_game_name_txt" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@drawable/di_rect_rounded_button"
        android:onClick="Play_Cup_Game"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="@+id/textView8"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_play_game" />

    <TextView
        android:id="@+id/music_game_name_txt"
        android:layout_width="181dp"
        android:layout_height="75dp"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="500dp"
        android:background="@drawable/di_rect_rounded_text"
        android:elegantTextHeight="false"
        android:onClick="Play_Pattern_Game"
        android:text="Match the Pattern "
        android:gravity="center"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Player_Name_txt"
        app:layout_constraintVertical_bias="0.74" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="129dp"
        android:layout_height="13dp"
        app:constraint_referenced_ids="music_game_name_txt"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="48dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/di_rect_rounded_button"
        android:onClick="Exit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_close" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/di_rect_rounded_button"
        android:onClick="LogOut"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_logout" />

    <TextView
        android:id="@+id/Player_Name_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/di_player_name_menu"
        android:text="Player Name"
        android:gravity="center"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>